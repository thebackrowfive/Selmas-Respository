<h1> in index.html file for credentials to only be viewed by administrators </h1>

<table>
  <tr>
      <th>User Name</th>
      <th>Login Question</th>
      <th></th>
      <th></th>
      <th></th>
  </tr>

  <% @credentials.each do |f| %>
    <tr>
       <td><%= f.userName %></td>
       <td><%= f.passQuestion %></td>
    
       <td><%= link_to 'Show', credential_path(f) %></td>
       <td><%= link_to 'Edit', edit_credential_path(f) %></td>
       <td><%= link_to 'Destroy', credential_path(f), method: :delete, data:{confirm: 'Are you sure?'} %> </td>
    </tr>

  <% end %>
  </table>

